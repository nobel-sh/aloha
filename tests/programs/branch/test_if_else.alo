import "stdlib/io.alo";

fun can_vote(age:number)-> bool {
    if (age < 18) {
       return false;
    }
    return true;
}

fun main() -> number {
    imut age:number = 16;
    if (can_vote(age)) {
       println("Can vote");
    }
    else {
        println("Can not vote");
    }
    return 0;
}
