import "stdlib/io.alo";

fun is_valid(n: number) -> number {
    if (n == 42) {
        return 1;
    }
    return 0;
}

fun main() -> number {
    imut value = 42;
    
    if (is_valid(value) == 1) {
        println("Test passed!");
        return 0;
    } else {
        println("Test failed!");
        return 1;
    }
}
