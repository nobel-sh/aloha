import "stdlib/io.alo";

struct Point {
  x: number,
  y: number,
}

struct Rectangle {
    width: number,
    height: number,
}

fun main() -> number {
    imut point = Point{10, 20};
    imut rect = Rectangle{100, 200};

    imut px = point->x;
    imut py = point->y;

    imut rw = rect->width;
    imut rh = rect->height;

    println("Point coordinates:");
    print("x = ");
    printlnNum(px);
    print("y = ");
    printlnNum(py);

    println("Rectangle dimensions:");
    print("width = ");
    printlnNum(rw);
    print("height = ");
    printlnNum(rh);

    return 0;
}
