import "stdlib/io.alo";

fun hello() -> void {
    imut msg: string = "Hello from extern test!";
    imut len: number = aloha_sys_strlen(msg);
    aloha_sys_write(1, msg, len);
    aloha_sys_write(1, "\n", 1);
}

fun main() -> number {
    hello();
    println("Now using stdlib function!");
    return 0;
}
